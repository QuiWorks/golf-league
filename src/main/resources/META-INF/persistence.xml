<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="golf_league">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>com.ejp.golf.league.domain.Course</class>
        <class>com.ejp.golf.league.domain.Event</class>
        <class>com.ejp.golf.league.domain.EventType</class>
        <class>com.ejp.golf.league.domain.Flight</class>
        <class>com.ejp.golf.league.domain.Golfer</class>
        <class>com.ejp.golf.league.domain.Hole</class>
        <class>com.ejp.golf.league.domain.League</class>
        <class>com.ejp.golf.league.domain.Nine</class>
        <class>com.ejp.golf.league.domain.GolferHandicap</class>
        <class>com.ejp.golf.league.domain.Round</class>
        <class>com.ejp.golf.league.domain.Score</class>
        <class>com.ejp.golf.league.domain.Season</class>
        <class>com.ejp.golf.league.domain.Team</class>
<!--        <class>com.ejp.golf.league.domain.TeamMatch</class>-->
        <class>com.ejp.golf.league.domain.TeamMember</class>
        <class>com.ejp.golf.league.domain.Admin</class>
        <properties>
            <property name="javax.persistence.dialect" value="org.hibernate.dialect.MySQL8Dialect"/>
            <property name="javax.persistence.jdbc.user" value="eric"/>
            <property name="javax.persistence.jdbc.password" value="password"/>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/golf_league"/>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
        </properties>
    </persistence-unit>
</persistence>